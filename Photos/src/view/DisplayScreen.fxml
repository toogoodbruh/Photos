<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.DisplayScreenController"
            >
    <children>
        <VBox prefHeight="400.0" prefWidth="640.0">
            <children>
                <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Button fx:id="QuitButton" layoutX="574.0" layoutY="360.0" mnemonicParsing="false" onAction="#QuitHandler" prefHeight="35.0" prefWidth="52.0" text="Quit" textAlignment="CENTER" />
                        <Button fx:id="LogOutButton" layoutX="507.0" layoutY="14.0" mnemonicParsing="false" onAction="#LogOutHandler" prefHeight="35.0" prefWidth="100.0" text="Log Out" textAlignment="CENTER" wrapText="true" />
                        <Button fx:id="BackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backHandler" text="Back" />
                        <TextField fx:id="Date" editable="false" layoutX="14.0" layoutY="360.0" prefHeight="25.0" prefWidth="197.0" />
                        <TextField fx:id="Caption" editable="false" layoutX="425.0" layoutY="85.0" prefHeight="25.0" prefWidth="197.0" />
                        <ListView fx:id="tagsListView" layoutX="425.0" layoutY="120.0" prefHeight="196.0" prefWidth="197.0" />
                        <ImageView fx:id="displayImageView" fitHeight="300.0" fitWidth="400.0" layoutX="14.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true" />
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>

</AnchorPane>
